    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..nbaliyan'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..-->>>--..nbaliyan'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying..-.-.-..nbaliyan'
            }
        }
    }
}
